<div class="page-header">
  <h3>Job List #<%= @job_list.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Job type</dt>
      <dd><%= @job_list.job_type_id %></dd>

      <dt>Job</dt>
      <dd><%= @job_list.job_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/job_lists" class="btn btn-primary">
        Back
      </a>
      <a href="/job_lists/<%= @job_list.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_job_list/<%= @job_list.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
