<div class="page-header">
  <h3>Feed #<%= @feed.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Job</dt>
      <dd><%= @feed.job_id %></dd>

      <dt>Comment</dt>
      <dd><%= @feed.comment %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/feeds" class="btn btn-primary">
        Back
      </a>
      <a href="/feeds/<%= @feed.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_feed/<%= @feed.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
